{
  "expo": {
    "name": "idonneoapp",
    "slug": "idonneoapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.idonneoapp.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.idonneoapp.app",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "idonneoapp",
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": "./app"
      },
      "eas": {
        "projectId": "4c2ecf8a-e6f5-487f-9dc3-12b80aa50e70"
      }
    }
  }
}